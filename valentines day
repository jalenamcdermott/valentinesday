<script> 
        var love_count = 0;
        var nolove_count = 0;
<!--adds one when corresponding button is pressed.-->
	
        function loveAdd(){
            love_count = love_count + 1;
		
        }

        function noloveAdd(){
            nolove_count = nolove_count + 1;
        }
	
	function results(){
		var lovePercent = (love_count/4.0) * 100;
		var nolovePercent = (nolove_count/4.0) * 100;
		var resultText = document.getElementById("results"); //captures the results and when 'results' button is pressed
		resultText.innerHTML = "Probability Jalena is madly in love with me =" + lovePercent + "%" + "<br> Probability Jalena hates me so so much bleck =" + nolovePercent + "%";
		<!-- imputs results on page using IDs; <br> means line break; printed variable with "+" -->
	}
	
  </script>

